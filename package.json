{
  "name": "qazuor-react-hooks",
  "version": "1.0.0",
  "description": "Paquete de hooks personalizados para React",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "react",
    "hooks",
    "typescript"
  ],
  "author": "Leandro Asrilevich 'Qazuor'",
  "license": "MIT",
  "packageManager": "pnpm@10.5.2",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.tsx --format cjs,esm --dts",
    "tests": "jest --coverage",
    "prepare": "husky",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:ci": "biome ci .",
    "lint:fix": "biome check --write --unsafe .",
    "lint-staged": "biome check . --write --no-errors-on-unmatched --files-ignore-unknown=true"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qazuor/reactCustomHooks.git"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsup": "^6.2.3",
    "typescript": "^5.8.2"
  }
}
